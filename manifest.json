{
  "manifest_version": 3,
  "version": "1.2.4",
  "name": "ManIIIFestor",
  "description": "Add IIIF-powered images from State Library Victoria's copyright-free image pool to your browser's newly opened tab.",
  "icons": {
    "16": "assets/images/state-library-shelf-icon-square-16.png",
    "48": "assets/images/state-library-shelf-icon-square-48.png",
    "128": "assets/images/state-library-shelf-icon-square-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "slv-iiif-image-tab.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "slv-lab-ManIIIFestor@example.com",
      "strict_min_version": "109.0"
    }
  }
}
